{
   "name": "@neuralline/tidy",
   "version": "0.0.1",
   "description": "Removes all node modules and build time files from your project directories and sub directories",
   "main": "dist/tidy.min",
   "input": "src/cli.ts",
   "cjs": "dist/tidy.cjs.js",
   "module": "dist/tidy.esm.js",
   "minified": "dist/tidy.min.js",
   "bin": {
      "tidy": "./bin/tidy-cli"
   },
   "publishConfig": {
      "access": "public"
   },
   "scripts": {
      "start": "node ./bin/tidy-cli",
      "tidy": "npx @neuralline/tidy",
      "build": "rollup -c ",
      "watch": "rollup -c -w",
      "test": "jest",
      "tsc": "tsc"
   },
   "keywords": [
      "tidy",
      "clean project",
      "remove npm dependencies",
      "uninstall",
      "delete",
      "remove node_modules",
      "yarn uninstall",
      "yarn remove",
      "npm uninstall",
      "delete node_modules",
      "clear cache",
      "tidy cli",
      "cli"
   ],
   "author": "Darik Hart",
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/neuralline/tidy.git"
   },
   "devDependencies": {
      "@rollup/plugin-commonjs": "^11.1.0",
      "@rollup/plugin-typescript": "^4.1.1",
      "@types/node": "^13.11.1",
      "jest": "^25.3.0",
      "rollup": "^2.6.1",
      "rollup-plugin-terser": "^5.3.0",
      "rollup-plugin-typescript2": "^0.27.0",
      "tslib": "^1.11.1",
      "typescript": "^3.8.3"
   },
   "dependencies": {
      "fs-extra": "^9.0.0",
      "inquirer": "^7.1.0",
      "listr": "^0.14.3"
   },
   "files": [
      "LICENSE",
      "README.md",
      "/bin",
      "/dist"
   ]
}
